> 14, 1, BCCEIEN ; UNLZ4 block checksum check error interrupt enable.
= 0, DISABLE
= 1, ENABLE

> 13, 1, HCCEIEN ; UNLZ4 header checksum check error interrupt enable.
= 0, DISABLE
= 1, ENABLE

> 12, 1, CSEIEN ; UNLZ4 content size (or GZIP isize) error interrupt enable.
= 0, DISABLE
