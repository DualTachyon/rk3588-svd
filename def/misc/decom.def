> 5, 1, LENEIEN ; INFLATE huffman length error interrupt enable.
= 0, DISABLE
= 1, ENABLE

> 4, 1, LITEIEN ; INFLATE huffman literal error interrupt enable.
= 0, DISABLE
= 1, ENABLE

> 3, 1, SQMEIEN ; INFLATE SQ match error interrupt enable.
= 0, DISABLE
