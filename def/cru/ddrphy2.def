[CRU_DDRPHY2, CRU]
@ = 0x0FD808000, 0x00004000

D2APLL_CON0 = 0x0000 ; D2APLL configuration register 0
> 15, 1, D2APLL_BP ; BYPASS: Bypass mode control signal.
= 1, BYPASS ; bypass mode is enabled. (FOUT = FIN).
= 0, NORMAL ; PLL operates normally.

> 0, 10, D2APLL_M ; M: Division value of the 10-bit programmable main-divider. PLL has to be reset if M value is changed. 64 <= pll_m <= 1023

D2APLL_CON1 = 0x0004 ; D2APLL configuration register 1
> 13, 1, D2APLL_RESETB ; RESETB: Power down control signal.
= 0, NORMAL ; RESETB=0 from 1, PLL starts its normal operation after lock time.
= 1, POWER_DOWN ; RESETB=1, power down mode is enabled and all digital blocks are reset.

> 6, 3, D2APLL_S ; Division value of the 3-bit programmable scaler. 0 <= pll_s <= 6
> 0, 6, D2APLL_P ; P: Division value of the 6-bit programmable pre-divider. PLL has to be reset if P value is changed. 1 <= pll_p <= 63

D2APLL_CON2 = 0x0008 ; D2APLL configuration register 2
> 0, 16, D2APLL_K ; K: Value of 16-bit DSM. pll_k[15:0] is a two's complement integer.

D2APLL_CON3 = 0x000C ; D2APLL configuration register 3
> 14, 2, D2APLL_SEL_PF ; SEL_PF: Value of 2-bit modulation method control.
= 0, DOWN_SPREAD
= 1, UP_SPREAD
= 2, CENTER_SPREAD2 ; PLL has to be reset if pll_sel_pf is changed.
= 3, CENTER_SPREAD3 ; PLL has to be reset if pll_sel_pf is changed.

> 8, 6, D2APLL_MRR ; MRR: Value of 6-bit modulation rate control. PLL has to be reset if pll_mrr is changed.
> 0, 8, D2APLL_MFR ; MFR: Value of 8-bit modulation frequency control. PLL has to be reset if pll_mfr is changed.

D2APLL_CON4 = 0x0010 ; D2APLL configuration register 4
> 15, 1, D2APLL_FSEL ; FSEL: Monitoring pin.
= 0, FREF ; FEED_OUT = FREF.
= 1, FEED ; FEED_OUT = FEED.

> 14, 1, D2APLL_FEED_EN ; FEED_EN: Monitoring pin.
= 0, DISABLED ; FEED_OUT is disabled.
= 1, ENABLED ; FEED_OUT is enabled.

> 4, 5, D2APLL_EXTAFC ; EXTAFC: Monitoring pin. If pll_afc_enb=1, AFC is disabled and VCO is calibrated manually by pll_extafc[4:0] for the test of VCO range.

> 3, 1, D2APLL_AFC_ENB ; AFC_ENB: Monitoring pin.
= 0, ENABLED ; AFC is enabled and VCO is calibrated automatically.
= 1, DISABLED ; AFC is disabled and VCO is calibrated manually by pll_extafc[4:0] for the test of VCO range.

> 0, 1, D2APLL_SSCG_EN ; SSCG_EN: Enable pin for dithered mode.
= 0, DISABLE ; disable dithered mode.
= 1, ENABLE ; enable dithered mode.

D2APLL_CON5 = 0x0014 ; D2APLL configuration register 5
> 0, 1, D2APLL_FOUT_MASK ; FOUT_MASK: Scaler's re-initialization time control pin.

D2APLL_CON6 = 0x0018 ; D2APLL configuration register 6
> 15, 1, D2APLL_LOCK ; LOCK: PLL lock flag.
= 0, UNLOCKED ; PLL is unlocked.
= 1, LOCKED ; PLL is locked.

> 10, 5, D2APLL_AFC_CODE ; AFC_CODE: Monitoring pin. Output code of AFC(5 bits).

D2BPLL_CON0 = 0x0020 ; D2BPLL configuration register 0
> 15, 1, D2BPLL_BP ; BYPASS: Bypass mode control signal.
= 1, BYPASS ; bypass mode is enabled. (FOUT = FIN).
= 0, NORMAL ; PLL operates normally.

> 0, 10, D2BPLL_M ; M: Division value of the 10-bit programmable main-divider. PLL has to be reset if M value is changed. 64 <= pll_m <= 1023

D2BPLL_CON1 = 0x0024 ; D2BPLL configuration register 1
> 13, 1, D2BPLL_RESETB ; RESETB: Power down control signal.
= 0, NORMAL ; RESETB=0 from 1, PLL starts its normal operation after lock time.
= 1, POWER_DOWN ; RESETB=1, power down mode is enabled and all digital blocks are reset.

> 6, 3, D2BPLL_S ; Division value of the 3-bit programmable scaler. 0 <= pll_s <= 6
> 0, 6, D2BPLL_P ; P: Division value of the 6-bit programmable pre-divider. PLL has to be reset if P value is changed. 1 <= pll_p <= 63

D2BPLL_CON2 = 0x0028 ; D2BPLL configuration register 2
> 0, 16, D2BPLL_K ; K: Value of 16-bit DSM. pll_k[15:0] is a two's complement integer.

D2BPLL_CON3 = 0x002C ; D2BPLL configuration register 3
> 14, 2, D2BPLL_SEL_PF ; SEL_PF: Value of 2-bit modulation method control.
= 0, DOWN_SPREAD
= 1, UP_SPREAD
= 2, CENTER_SPREAD2 ; PLL has to be reset if pll_sel_pf is changed.
= 3, CENTER_SPREAD3 ; PLL has to be reset if pll_sel_pf is changed.

> 8, 6, D2BPLL_MRR ; MRR: Value of 6-bit modulation rate control. PLL has to be reset if pll_mrr is changed.
> 0, 8, D2BPLL_MFR ; MFR: Value of 8-bit modulation frequency control. PLL has to be reset if pll_mfr is changed.

D2BPLL_CON4 = 0x0030 ; D2BPLL configuration register 4
> 15, 1, D2BPLL_FSEL ; FSEL: Monitoring pin.
= 0, FREF ; FEED_OUT = FREF.
= 1, FEED ; FEED_OUT = FEED.

> 14, 1, D2BPLL_FEED_EN ; FEED_EN: Monitoring pin.
= 0, DISABLED ; FEED_OUT is disabled.
= 1, ENABLED ; FEED_OUT is enabled.

> 4, 5, D2BPLL_EXTAFC ; EXTAFC: Monitoring pin. If pll_afc_enb=1, AFC is disabled and VCO is calibrated manually by pll_extafc[4:0] for the test of VCO range.

> 3, 1, D2BPLL_AFC_ENB ; AFC_ENB: Monitoring pin.
= 0, ENABLED ; AFC is enabled and VCO is calibrated automatically.
= 1, DISABLED ; AFC is disabled and VCO is calibrated manually by pll_extafc[4:0] for the test of VCO range.

> 0, 1, D2BPLL_SSCG_EN ; SSCG_EN: Enable pin for dithered mode.
= 0, DISABLE ; disable dithered mode.
= 1, ENABLE ; enable dithered mode.

D2BPLL_CON5 = 0x0034 ; D2BPLL configuration register 5
> 0, 1, D2BPLL_FOUT_MASK ; FOUT_MASK: Scaler's re-initialization time control pin.

D2BPLL_CON6 = 0x0038 ; D2BPLL configuration register 6
> 15, 1, D2BPLL_LOCK ; LOCK: PLL lock flag.
= 0, UNLOCKED ; PLL is unlocked.
= 1, LOCKED ; PLL is locked.

> 10, 5, D2BPLL_AFC_CODE ; AFC_CODE: Monitoring pin. Output code of AFC(5 bits).

CLKSEL_CON0 = 0x0300 ; Internal clock select and division register 0
> 0, 1, CLK_DDRPHY2X_CH2_SEL ; clk_ddrphy2x_ch2 clock mux.
= 0, CLK_D2APLL_T
= 1, CLK_D2BPLL

GATE_CON00 = 0x0800 ; Internal clock gate and division register 0
> 5, 1, CLK_OSC_DDRPHY_CH2_EN ; clk_osc_ddrphy_ch2 clock gating control.
= 0, ENABLE
= 1, DISABLE

> 4, 1, PCLK_DDRPHY_CH2_EN ; pclk_ddrphy_ch2 clock gating control.
= 0, ENABLE
= 1, DISABLE

> 3, 1, PCLK_DDR_CRU_CH2_EN ; pclk_ddr_cru_ch2 clock gating control.
= 0, ENABLE
= 1, DISABLE

SOFTRST_CON00 = 0x0A00 ; Internal clock reset register 0
> 4, 1, PRESETN_DDRPHY_CH2 ; When high, reset relative logic
> 3, 1, PRESETN_DDR_CRU_CH2 ; When high, reset relative logic
> 2, 1, RESETN_DDRPHY2X_CH2 ; When high, reset relative logic
> 1, 1, RESETN_DDRPHY2XDIV_CH2 ; div_rst_n at ddr ch2 When high, reset relative logic

